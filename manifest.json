{
    "manifest_version": 2,
    "description": "Chrome Extention that blocks your favorite social media sites, so you can do less scrolling and more studying.",
    "name": "Social Media Blocker", 
    "version" : "0.1.0",
    "content_scripts": [ 
        {

        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css" : ["style.css"]

       }

    ]
    ,"permissions": [
        "webrequest",
        "*://www.twitter.com/*",
        "webRequestBlocking"

    ]
    
}
